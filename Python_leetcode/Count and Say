class Solution:
    def countAndSay(self, n: int) -> str:
        arr = [''] * (n+1)
        arr[1] = '1'
        for i in range(2,n+1):
            prev = arr[i-1]
            curr=''
            j=0
            while j<len(prev):
                count=1
                while j+1<len(prev) and prev[j]==prev[j+1]:
                    count+=1
                    j+=1
                curr += str(count)+prev[j]
                j+=1
            arr[i]=curr
        return arr[n]
        
